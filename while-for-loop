public class HelloWorld{
	public static void main(String[] args){
	
	int x = 1;
	
      while  ( x <= 100){
	
          System.out.println(5*x);
	
            x=2*x;
	
           }                    
  
  for (int y = 1; y<=5; y++) {
	 
        System.out.println(y*2);
	      } 
    }	
  
  }

              
